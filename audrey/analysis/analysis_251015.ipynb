    "    }\n",
    "   },\n",
    "   \"source\": [\n",
    "    \"##%% optimized single-panel firing rate plotting\\n\",\n",
    "    \"DEBUG = False   # set to False to mute debug output\\n\",\n",
    "    \"\\n\",\n",
    "    \"def dbg(*msg):\\n\",\n",
    "    \"    if DEBUG:\\n\",\n",
    "    \"        print(\\\"[DEBUG]\\\", *msg)\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"x  = 0\\n\",\n",
    "    \"plot_titles = [\\n\",\n",
    "    \"    'PA',\\n\",\n",
    "    \"    'PA + light',\\n\",\n",
    "    \"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"for cluster_id in data_io.cluster_df.index.values:\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # x += 1\\n\",\n",
    "    \"    # if x == 3:\\n\",\n",
    "    \"    #     break\\n\",\n",
    "    \"\\n\",\n",
    "    \"    dbg(f\\\"\\\\n=== Processing cluster {cluster_id} ===\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Create a *single panel* figure\\n\",\n",
    "    \"    fig = utils.make_figure(\\n\",\n",
    "    \"        width    = 1,\\n\",\n",
    "    \"        height   = 1.5,\\n\",\n",
    "    \"        x_domains={1: [[0.1, 0.5], [0.6, 0.9]],\\n\",\n",
    "    \"                   2: [[0.1, 0.4], [0.6, 0.9]]},\\n\",\n",
    "    \"        y_domains={1: [[0.6, 0.9], [0.6, 0.9]],\\n\",\n",
    "    \"                   2: [[0.1, 0.4], [0.1, 0.4]]},\\n\",\n",
    "    \"        subplot_titles={\\n\",\n",
    "    \"            1: ['8', '12'],\\n\",\n",
    "    \"            2: ['12' ,'29']\\n\",\n",
    "    \"        }\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Always plot into (row=1, col=1)\\n\",\n",
    "    \"    pos = dict(row=1, col=1)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Load data for this cluster\\n\",\n",
    "    \"    dbg(\\\"Loading bootstrap dataâ€¦\\\")\\n\",\n",
    "    \"    cluster_data = utils.load_obj(\\n\",\n",
    "    \"        data_dir / 'bootstrapped' / f'bootstrap_{cluster_id}.pkl'\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # preferred electrode\\n\",\n",
    "    \"    ec = pref_ec_dict[cluster_id]\\n\",\n",
    "    \"    dbg(\\\"Preferred electrode:\\\", ec)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if ec is None:\\n\",\n",
    "    \"        dbg(\\\"Skipping cluster (no preferred electrode)\\\")\\n\",\n",
    "    \"        continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Track global y-limits\\n\",\n",
    "    \"    y_max = 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Loop over all recordings (all curves go on same axis)\\n\",\n",
    "    \"    for rec_i, rec_name in enumerate(selected_rec_names):\\n\",\n",
    "    \"\\n\",\n",
    "    \"        dbg(f\\\"  Recording: {rec_name}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # Filter data\\n\",\n",
    "    \"        d_select = data_io.burst_df.query(\\n\",\n",
    "    \"            'electrode == @ec and recording_name == @rec_name'\\n\",\n",
    "    \"        ).copy()\\n\",\n",
    "    \"\\n\",\n",
    "    \"        dbg(\\\"    Burst trials found:\\\", len(d_select))\\n\",\n",
    "    \"\\n\",\n",
    "    \"        if len(d_select) == 0:\\n\",\n",
    "    \"            dbg(\\\"    WARNING: No trials match this electrode/recording\\\")\\n\",\n",
    "    \"            continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"        d_select.sort_values('duty_cycle', inplace=True)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # Determine condition type: PA vs PA+light\\n\",\n",
    "    \"        if '_light_pa_' in rec_name:\\n\",\n",
    "    \"            duty_cycles = d_select.laser_duty_cycle.unique()\\n\",\n",
    "    \"            dbg(\\\"    Condition detected: PA + light\\\")\\n\",\n",
    "    \"            mode = 'light'\\n\",\n",
    "    \"        elif '_pa_' in rec_name:\\n\",\n",
    "    \"            duty_cycles = d_select.duty_cycle.unique()\\n\",\n",
    "    \"            dbg(\\\"    Condition detected: PA\\\")\\n\",\n",
    "    \"            mode = 'pa'\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            dbg(\\\"    WARNING: Unrecognized condition; skipping\\\")\\n\",\n",
    "    \"            continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"        dbg(\\\"    Duty cycles:\\\", duty_cycles)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        for dc in duty_cycles:\\n\",\n",
    "    \"\\n\",\n",
    "    \"            if dc == 8:\\n\",\n",
    "    \"                pos = dict(row=1, col=1)\\n\",\n",
    "    \"            elif dc == 12:\\n\",\n",
    "    \"                pos = dict(row=1, col=2)\\n\",\n",
    "    \"            elif dc == 20:\\n\",\n",
    "    \"                pos = dict(row=2, col=1)\\n\",\n",
    "    \"            elif dc == 29:  \\n\",\n",
    "    \"                pos = dict(row=2, col=2)\\n\",\n",
    "    \"            dbg(f\\\"      Duty cycle: {dc}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # Extract train ID + burst settings\\n\",\n",
    "    \"            if mode == 'light':\\n\",\n",
    "    \"                row0 = d_select.query('laser_duty_cycle == @dc').iloc[0]\\n\",\n",
    "    \"                tid  = row0.train_id\\n\",\n",
    "    \"\\n\",\n",
    "    \"                bdf  = data_io.burst_df\\n\",\n",
    "    \"                frep = bdf.query('train_id == @tid').iloc[0].laser_duty_cycle\\n\",\n",
    "    \"                bd   = bdf.query('train_id == @tid').iloc[0].laser_burst_duration\\n\",\n",
    "    \"\\n\",\n",
    "    \"                \\n\",\n",
    "    \"                \\n\",\n",
    "    \"                clra_a = 'rgba(255, 165, 0, 0.2)'  # orange, alpha 0.2\\n\",\n",
    "    \"                clr = 'rgba(255, 165, 0, 1)'      # orange, alpha 1\\n\",\n",
    "    \"\\n\",\n",
    "    \"            elif mode == 'pa':\\n\",\n",
    "    \"                row0 = d_select.query('duty_cycle == @dc').iloc[0]\\n\",\n",
    "    \"                tid  = row0.train_id\\n\",\n",
    "    \"\\n\",\n",
    "    \"                bdf  = data_io.burst_df\\n\",\n",
    "    \"                frep = bdf.query('train_id == @tid').iloc[0].duty_cycle\\n\",\n",
    "    \"                bd   = bdf.query('train_id == @tid').iloc[0].burst_duration\\n\",\n",
    "    \"\\n\",\n",
    "    \"                # clr_a = clrs.duty_cycle(dc, alpha = 0.2)\\n\",\n",
    "    \"                # clr   = clrs.duty_cycle(dc)\\n\",\n",
    "    \"                clra_a = 'rgba(0, 128, 0, 0.2)'    # green, alpha 0.2\\n\",\n",
    "    \"                clr = 'rgba(0, 128, 0, 1)'        #\\n\",\n",
    "    \"\\n\",\n",
    "    \"            dbg(\\\"        train_id:\\\", tid, \\\"| burst dur:\\\", bd)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # Load bootstrap FR data\\n\",\n",
    "    \"            bins    = cluster_data[tid]['bins']\\n\",\n",
    "    \"            sp      = cluster_data[tid]['firing_rate']\\n\",\n",
    "    \"            ci_low  = cluster_data[tid]['firing_rate_ci_low']\\n\",\n",
    "    \"            ci_high = cluster_data[tid]['firing_rate_ci_high']\\n\",\n",
    "    \"\\n\",\n",
    "    \"            if sp is None:\\n\",\n",
    "    \"                dbg(\\\"        WARNING: No spike data for tid\\\", tid)\\n\",\n",
    "    \"                continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"            y_max = max(y_max, ci_high.max())\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # --- shaded burst window ---\\n\",\n",
    "    \"            fig.add_scatter(\\n\",\n",
    "    \"                x=[0, 0, bd, bd],\\n\",\n",
    "    \"                y=[0, 1000, 1000, 0],\\n\",\n",
    "    \"                mode='lines',\\n\",\n",
    "    \"                line=dict(width=0),\\n\",\n",
    "    \"                fill='toself',\\n\",\n",
    "    \"                fillcolor='rgba(128, 128, 128, 0.1)',\\n\",\n",
    "    \"                showlegend=False,\\n\",\n",
    "    \"                **pos,\\n\",\n",
    "    \"            )\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # --- confidence interval ---\\n\",\n",
    "    \"            fig.add_scatter(\\n\",\n",
    "    \"                x=bins,\\n\",\n",
    "    \"                y=ci_low,\\n\",\n",
    "    \"                line=dict(width=0),\\n\",\n",
    "    \"                showlegend=False,\\n\",\n",
    "    \"                **pos,\\n\",\n",
    "    \"            )\\n\",\n",
    "    \"            fig.add_scatter(\\n\",\n",
    "    \"                x=bins,\\n\",\n",
    "    \"                y=ci_high,\\n\",\n",
    "    \"                mode='lines',\\n\",\n",
    "    \"                line=dict(width=0),\\n\",\n",
    "    \"                fill='tonexty',\\n\",\n",
    "    \"                fillcolor='rgba(128, 128, 128, 0.1)',\\n\",\n",
    "    \"                showlegend=False,\\n\",\n",
    "    \"                **pos,\\n\",\n",
    "    \"            )\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # --- firing-rate line ---\\n\",\n",
    "    \"            legend_name = f\\\"DC {dc}\\\"\\n\",\n",
    "    \"            show_leg = not any(trace.name == legend_name for trace in fig.data)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            fig.add_scatter(\\n\",\n",
    "    \"                x=bins,\\n\",\n",
    "    \"                y=sp,\\n\",\n",
    "    \"                name=mode,\\n\",\n",
    "    \"                line=dict(width=1, color=clr),\\n\",\n",
    "    \"                showlegend=show_leg,\\n\",\n",
    "    \"                **pos,\\n\",\n",
    "    \"            )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Final axes formatting\\n\",\n",
    "    \"    for i in range(1, 3):\\n\",\n",
    "    \"        for j in range(1, 3):\\n\",\n",
    "    \"            fig.update_xaxes(\\n\",\n",
    "    \"                tickvals=np.arange(-500, 500, 100),\\n\",\n",
    "    \"                title_text='Time (ms)',\\n\",\n",
    "    \"                range=[-101, 351],\\n\",\n",
    "    \"                row=i, col=j,\\n\",\n",
    "    \"            )\\n\",\n",
    "    \"            fig.update_yaxes(\\n\",\n",
    "    \"                title_text='Firing rate (Hz)',\\n\",\n",
    "    \"                tickvals=np.arange(0, np.ceil(y_max/10)*10 + 1, 10),  # nice 10-Hz spacing\\n\",\n",
    "    \"                range=[0, y_max],\\n\",\n",
    "    \"                row=i, col=j,\\n\",\n",
    "    \"            )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Save figure\\n\",\n",
    "    \"    sname = figure_dir / 'firing_rate_per_condition' / f'{cluster_id}'\\n\",\n",
    "    \"    dbg(\\\"Saving figure to:\\\", sname)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    utils.save_fig(fig, sname, display=False, formats=['png', 'svg'])\\n\",\n",
    "    \"\\n\",\n",
    "    \"print(\\\"\\\\n\\\\nAll clusters processed.\\\\n\\\\t\\\\t\\\\t------ End Of Cell ------\\\")\\n\"\n",
